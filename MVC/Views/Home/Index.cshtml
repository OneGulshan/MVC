@model IEnumerable<MVC.Models.Employee>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table>
    <tr>
        <th>Employee ID</th>
        <th>Employee Name</th>
        <th>Employee Age</th>
        <th>Employee Email</th>
        <th>Employee Organisation</th>
        <th>Address</th>
        <th>Joining Date</th>
        <th>Joining Time</th>
    </tr>

    @{ 
        foreach (var item in Model)
        {
            <tr>
                <td>@item.EmployeeId</td>
                <td>@item.EmployeeName</td>
                <td>@item.EmployeeAge</td>
                <td>@item.EmployeeEmail</td>
                <td>@item.EmpOrganisationName</td>
                <td>@item.Address</td>
                <td>@item.JoningDate</td>
                <td>@item.JoiningTime</td>
            </tr>
        }
    }
</table>